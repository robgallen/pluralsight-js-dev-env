{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node build/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch",
    "open:src": "babel-node build/serve.js",
    "lint": "esw webpack.config.* src build --color; exit 0;",
    "lint:watch": "esw --watch webpack.config.* src build --color; exit 0;",
    "security-check": "nsp check"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "2.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-preset-latest": "6.22.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "eslint": "3.15.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-watch": "3.0.0",
    "express": "4.14.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.28.0",
    "jsdom": "9.11.0",
    "json-schema-faker": "0.4.0",
    "json-server": "0.9.5",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "npm-run-all": "4.0.1",
    "nsp": "2.6.2",
    "numeral": "2.0.4",
    "open": "0.0.5",
    "rimraf": "2.6.0",
    "style-loader": "0.13.1",
    "surge": "0.18.0",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.1",
    "webpack-dev-server": "2.4.1",
    "webpack-hot-middleware": "2.17.0",
    "webpack-md5-hash": "0.0.5"
  }
}
